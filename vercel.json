{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/testnet/:path*",
      "destination": "https://demo-fapi.binance.com/:path*"
    },
    {
      "source": "/api/futures/:path*",
      "destination": "https://fapi.binance.com/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "https://api.binance.com/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
